   "source": [
    "year = int(input(\"Enter the year : \"))\n",
    "if (year % 100 == 0) and (year % 4 == 0) and (year % 400 == 0):\n",
    "    print(f\"{year} is a leap year.\")\n",
    "else:\n",
    "    print(f\"{year} is NOT a leap year.\")\n",
